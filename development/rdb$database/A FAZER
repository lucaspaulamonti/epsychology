
DAY, ROOM, IN, OUT, CUSTOMER, SERVICE, COLLABORATOR, SUPERVISOR, OBSERVATION;
MONDAY, ROOM 1, 08:00, 09:00, DAVI DOS SANTOS FERREIRA, PSICOTERAPIA CLINICA, LUCAS DE PAULA MONTI, CAMILLA CARRARI CRISTINA, CONFIRMADO;

CREATE TABLE TBPLANNER (
    ID_ SERIAL
    
    IDWEEK_ INTEGER NOT NULL,
    
    IDROOM_ INTEGER NOT NULL,
    IDIN_ INTEGER NOT NULL,
    IDOUT_ INTEGER NOT NULL,
    
    IDCUSTOMER_ INTEGER,
    IDSERVICE_ INTEGER,
    
    IDCOLLABORATOR_ INTEGER,
    IDSUPERVISOR_ INTEGER,
    
    OBSERVATION VARCHAR(255),

    FOREIGN KEY (IDWEEK_) REFERENCES TBWEEK (ID_),
    FOREIGN KEY (IDROOM_) REFERENCES TBROOM (ID_),
    FOREIGN KEY (IDIN_) REFERENCES TBTIME (ID_),

);

INSERT INTO TBPLANNER (IDWEEK_, IDROOM_, IDIN_, IDOUT_, IDCUSTOMER_, IDSERVICE_, IDCOLLABORATOR_, IDSUPERVISOR_, OBSERVATION) 
VALUES (1, 1, 33, 36, NULL, NULL, NULL, NULL, NULL);
